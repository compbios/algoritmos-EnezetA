Algoritmo ponderado
	
	Escribir "Ingrese su nota de primer parcial"
	Leer parcial1
	
	Escribir "Ingrese su nota de segundo parcial"
	Leer parcial2;
	
	Escribir "Ingrese su nota de trabajos"
	Leer participacion
	
	Escribir "Ingrese su nota de parcial final"
	Leer parcialFinal
	
	promedioFinal <- (parcial1 * 0.25 + parcial2 * 0.25 + participacion * 0.20 + parcialFinal * 0.3)
	Escribir "Su promedio final es: ", promedioFinal
	
	
FinAlgoritmo
